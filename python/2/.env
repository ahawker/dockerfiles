# Name of the image created from the Dockerfile.
IMAGE=python2

# Name of the base image to build on.
BASE_IMAGE=alpine

# Version of Python 2 to install.
PYTHON_VERSION=2.7.15

# Name of the Python 2 Alpine package to install.
ALPINE_PYTHON_PACKAGE_NAME=python

# Version of the Python 3 Alpine package to install.
ALPINE_PYTHON_PACKAGE_VERSION=2.7.15-r0
