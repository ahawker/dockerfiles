# Name of the image created from the Dockerfile.
IMAGE = maven-3.5.4

# Name of the base image to build on.
BASE_IMAGE = jre-8.181.13

# Version of Java to build on.
JAVA_VERSION = 8.181.13

# Version of Maven to install.
MAVEN_VERSION = 3.5.4

# Version of the Maven Alpine package to install.
ALPINE_MAVEN_PACKAGE_VERSION = $(MAVEN_VERSION)-r1
